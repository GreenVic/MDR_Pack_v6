  <peripherals>
    <!-- GPIO_PORTA -->
    <peripheral>
      <name>MDR_PORTA</name>
      <version>1.0</version>
      <description>GPIO Port Control</description>
      <groupName>GPIO Ports</groupName>
      <baseAddress>0x400A8000</baseAddress>
      <!--baseAddress>0x40080000</baseAddress><!--#BE4,#BK214,#BK234-->

      <size>32</size>
      <access>read-write</access>
      <addressBlock>
