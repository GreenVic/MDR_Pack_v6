              <enumeratedValues>
                <headerEnumName>MDR_GPIO__PIN_DOWN_</headerEnumName>
                <enumeratedValue>
                  <name>Off</name>
                  <description>No pull selected</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>DOWN</name>
                  <description>Pin pulled down to GND</description>
                  <value>1</value>
                </enumeratedValue>
