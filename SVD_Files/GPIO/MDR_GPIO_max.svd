              <bitRange>[1:0]</bitRange>
              <access>read-write</access>
              <enumeratedValues>
                <headerEnumName>MDR_GPIO__PIN_FUNC_</headerEnumName>
                <enumeratedValue>
                  <name>Port</name>
                  <description>RXTX defined pin</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Main</name>
                  <description>Main Periph defined pin</description>
                  <value>1</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Alter</name>
                  <description>Alter Periph defined pin</description>
                  <value>2</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Override</name>
                  <description>Override Periph defined pin</description>
                  <value>3</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
          </fields>
