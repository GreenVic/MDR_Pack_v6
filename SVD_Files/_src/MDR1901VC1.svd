                <headerEnumName>MDR_EEPROM_CMD__RD_</headerEnumName>
                <enumeratedValue>
                  <name>Inactive</name>
                  <description>No read operation</description>
                  <value>0</value>
                </enumeratedValue>
                <enumeratedValue>
                  <name>Active</name>
                  <description>Read operation</description>
                  <value>1</value>
                </enumeratedValue>
              </enumeratedValues>
            </field>
            <!-- #DELAY -->
